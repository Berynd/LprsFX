<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.ComboBox?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.VBox?>
<?import javafx.scene.text.Font?>

<VBox alignment="CENTER" prefHeight="690.0" prefWidth="382.0" spacing="20.0" xmlns="http://javafx.com/javafx/24.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="appli.accueil.InscriptionController">
    <padding>
        <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
    </padding>

    <Label fx:id="welcomeText" />
    <Label text="Bienvenue !">
        <font>
            <Font size="24.0" />
        </font>
    </Label>
    <Label fx:id="nom" text="Votre Nom :" />
    <TextField fx:id="nomText" />
    <Label fx:id="prenom" text="Votre Prénom" />
    <TextField fx:id="prenomText" />
    <Label fx:id="email" text="Votre Email :" />
    <TextField fx:id="emailText" />
    <Label fx:id="mdp" text="Choisissez un mot de passe : " />
    <PasswordField fx:id="mdpTexte" />
    <Label fx:id="mdpconfirmer" text="Confirmer le mot de passe :" />
    <PasswordField fx:id="mdpconfirmerTexte" />
   <ComboBox fx:id="roleCombo" prefHeight="25.0" prefWidth="169.0" promptText="Sélectionnez votre rôle" />
    <Label fx:id="erreur" textFill="RED" />
    <Button fx:id="inscription" onAction="#onHelloButtonClickInscription" text="Inscription" />
    <Button fx:id="connexion" onAction="#onHelloButtonClickConnexion" text="Connexion" />
</VBox>
